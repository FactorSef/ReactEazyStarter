    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-webpack-plugin": "^2.1.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.5.0",
    "prettier": "2.1.2",
    "ts-node": "^9.0.0",
    "type-fest": "^0.17.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
